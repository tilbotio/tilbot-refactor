{
	"name": "tilbot",
	"version": "2.0.0",
	"private": true,
	"main": "electron-main.cjs",
	"scripts": {
		"dev": "del-cli ./build && (copy svelte_server.config.js svelte.config.js || cp svelte_server.config.js svelte.config.js) && concurrently \"npm run server\" \"npm run dev-vite\"",
		"prod": "npm run build && npm run server",
		"server": "node server.js",
		"dev-vite": "vite dev --host 0.0.0.0 --port 5173",
		"electron": "(copy svelte_electron.config.js svelte.config.js || cp svelte_electron.config.js svelte.config.js) && vite build && electron .",
		"build": "(copy svelte_server.config.js svelte.config.js || cp svelte_server.config.js svelte.config.js) && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"package-win": "electron-packager . tilbot --overwrite --platform=win32 --arch=x64 --icon=src/icon/win/icon.ico",
		"package-mac-x64": "electron-packager . tilbot --overwrite --platform=darwin --arch=x64 --icon=src/icon/mac/icon.icns",
		"package-mac-arm": "electron-packager . tilbot --overwrite --platform=darwin --arch=arm64 --icon=src/icon/mac/icon.icns"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.1",
		"@sveltejs/kit": "next",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@types/crypto-js": "^4.1.3",
		"@types/papaparse": "^5.3.7",
		"autoprefixer": "^10.4.12",
		"concurrently": "^8.2.2",
		"del-cli": "^5.1.0",
		"electron": "^22.0.0",
		"electron-packager": "^17.1.1",
		"postcss": "^8.4.18",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.1",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.0.1",
		"adm-zip": "^0.5.10",
		"cors": "^2.8.5",
		"crypto-js": "^4.1.1",
		"csv-database": "^0.9.2",
		"daisyui": "^2.46.0",
		"express": "^4.18.2",
		"express-mongodb-session": "^3.2.1",
		"express-session": "^1.17.3",
		"html5-qrcode": "^2.3.7",
		"mongoose": "^6.6.7",
		"mongoose-bcrypt": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"n-readlines": "^1.0.1",
		"node-fetch": "^2.7.0",
		"openai": "^3.3.0",
		"papaparse": "^5.4.1",
		"public-ip": "4.0.4",
		"socket.io": "^4.5.4"
	}
}
