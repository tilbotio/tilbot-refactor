{
    "name": "tilbot-electron",
    "version": "1.0.0",
    "private": true,
    "main": "electron-main.cjs",
    "scripts": {
      "start": "electron .",
      "build": "cp svelte_electron.config.js ../apps/frontend/sveltekit/svelte.config.js",
      "package-win": "electron-packager . tilbot --overwrite --platform=win32 --arch=x64 --icon=src/icon/win/icon.ico",
      "package-mac-x64": "electron-packager . tilbot --overwrite --platform=darwin --arch=x64 --icon=frontend/sveltekit/src/icon/mac/icon.icns",
      "package-mac-arm": "electron-packager . tilbot --overwrite --platform=darwin --arch=arm64 --icon=frontend/sveltekit/src/icon/mac/icon.icns"
    },
    "devDependencies": {
      "adm-zip": "^0.5.10",
      "csv-database": "^0.9.2",
      "del-cli": "^5.1.0",
      "electron": "^22.0.0",
      "electron-packager": "^17.1.1",
      "node-fetch": "^2.7.0",
      "public-ip": "4.0.4",
      "tslib": "^2.3.1"
    }
  }
