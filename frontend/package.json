{
    "name": "frontend",
    "version": "2.0.0",
    "type": "module",
    "scripts": {
      "dev": "del-cli ./build && cp svelte_server.config.js svelte.config.js",
      "dev-vite": "vite dev",
      "build": "cp svelte_server.config.js svelte.config.js && npm run build:vite",
      "build:vite": "vite build",
      "preview": "vite preview",
      "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
      "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
    },
    "devDependencies": {
      "@sveltejs/adapter-auto": "next",
      "@sveltejs/adapter-static": "^1.0.1",
      "@sveltejs/kit": "next",
      "@tailwindcss/line-clamp": "^0.4.2",
      "@types/crypto-js": "^4.1.3",
      "@types/papaparse": "^5.3.7",
      "autoprefixer": "^10.4.12",
      "del-cli": "^5.1.0",
      "postcss": "^8.4.18",
      "svelte": "^3.44.0",
      "svelte-check": "^2.7.1",
      "svelte-preprocess": "^4.10.7",
      "tailwindcss": "^3.2.1",
      "typescript": "^4.7.4",
      "vite": "^4.0.0"
    },
    "dependencies": {
      "@sveltejs/adapter-node": "^1.0.1",
      "crypto-js": "^4.1.1",
      "daisyui": "^2.46.0",
      "html5-qrcode": "^2.3.7",
      "openai": "^3.3.0",
      "papaparse": "^5.4.1"
    }
  }
