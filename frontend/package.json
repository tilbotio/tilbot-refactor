{
    "name": "frontend",
    "version": "2.0.0",
    "type": "module",
    "scripts": {
        "dev": "del-cli ./build && cp svelte_server.config.js svelte.config.js",
        "dev-vite": "vite dev",
        "build": "cp svelte_server.config.js svelte.config.js && npm run build:vite",
        "build:vite": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^6.0.1",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.21.2",
        "@sveltejs/vite-plugin-svelte": "^5.1.0",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@types/crypto-js": "^4.1.3",
        "@types/papaparse": "^5.3.7",
        "autoprefixer": "^10.4.12",
        "del-cli": "^5.1.0",
        "postcss": "^8.4.18",
        "svelte": "^5.0.0",
        "svelte-check": "^3.4.3",
        "svelte-preprocess": "^6.0.3",
        "tailwindcss": "^3.2.1",
        "typescript": "^5.0.0",
        "vite": "^6.0.0"
    },
    "dependencies": {
        "@sveltejs/adapter-node": "^5.2.12",
        "crypto-js": "^4.1.1",
        "daisyui": "^2.46.0",
        "html5-qrcode": "^2.3.7",
        "openai": "^3.3.0",
        "papaparse": "^5.4.1"
    }
}
